{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "groups_4_x"
      }
   ],
   "group": "gamma",
   "label_template": "groups_4_x.${godot-template-groups-4-x_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "ignore_for_scheduling": false,
         "name": "godot-template-groups-4-x_pipeline_dependency",
         "pipeline": "godot-template-groups-4-x",
         "stage": "templateZipStage",
         "type": "dependency"
      }
   ],
   "name": "production-groups-release-export-4-x",
   "stages": [
      {
         "clean_workspace": false,
         "fetch_materials": true,
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "export_windows",
                     "type": "build"
                  }
               ],
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "defaultJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.templates.tpz",
                     "stage": "templateZipStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf templates && unzip \"godot.templates.tpz\" && mkdir pdbs && mv templates/*.pdb pdbs && export VERSION=\"`cat templates/version.txt`\" && export TEMPLATEDIR=\".local/share/godot/templates/$VERSION\" && export BASEDIR=\"`pwd`\" && rm -rf \"$TEMPLATEDIR\" && mkdir -p \"$TEMPLATEDIR\" && cd \"$TEMPLATEDIR\" && mv \"$BASEDIR\"/templates/* ."
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "export_linux_x11",
                     "type": "build"
                  }
               ],
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "defaultJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.templates.tpz",
                     "stage": "templateZipStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf templates && unzip \"godot.templates.tpz\" && mkdir pdbs && mv templates/*.pdb pdbs && export VERSION=\"`cat templates/version.txt`\" && export TEMPLATEDIR=\".local/share/godot/templates/$VERSION\" && export BASEDIR=\"`pwd`\" && rm -rf \"$TEMPLATEDIR\" && mkdir -p \"$TEMPLATEDIR\" && cd \"$TEMPLATEDIR\" && mv \"$BASEDIR\"/templates/* ."
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "export_macos",
                     "type": "build"
                  }
               ],
               "name": "macosJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "defaultJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.templates.tpz",
                     "stage": "templateZipStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf templates && unzip \"godot.templates.tpz\" && mkdir pdbs && mv templates/*.pdb pdbs && export VERSION=\"`cat templates/version.txt`\" && export TEMPLATEDIR=\".local/share/godot/templates/$VERSION\" && export BASEDIR=\"`pwd`\" && rm -rf \"$TEMPLATEDIR\" && mkdir -p \"$TEMPLATEDIR\" && cd \"$TEMPLATEDIR\" && mv \"$BASEDIR\"/templates/* ."
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "cd export_macos && unzip v_sekai_macos.zip && rm v_sekai_macos.zip"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "exportStage"
      },
      {
         "clean_workspace": false,
         "jobs": [
            {
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": false,
                     "job": "windowsJob",
                     "pipeline": "production-groups-release-export-4-x",
                     "source": "export_windows",
                     "stage": "exportStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_windows $ITCHIO_LOGIN:windows-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": false,
                     "job": "linuxDesktopJob",
                     "pipeline": "production-groups-release-export-4-x",
                     "source": "export_linux_x11",
                     "stage": "exportStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_linux_x11 $ITCHIO_LOGIN:x11-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "macosJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": false,
                     "job": "macosJob",
                     "pipeline": "production-groups-release-export-4-x",
                     "source": "export_macos",
                     "stage": "exportStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_macos $ITCHIO_LOGIN:macos --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "uploadStage"
      }
   ]
}
