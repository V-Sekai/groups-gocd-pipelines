{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "purple_gold_spitz"
      }
   ],
   "group": "gamma",
   "label_template": "purple_gold_spitz.${godot-template-groups-4-x_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "branch": "main",
         "destination": "g",
         "name": "project_git_sandbox",
         "type": "git",
         "url": "https://github.com/fire/godot-purple-gold-spitz.git"
      },
      {
         "ignore_for_scheduling": false,
         "name": "godot-template-groups-4-x_pipeline_dependency",
         "pipeline": "godot-template-groups-4-x",
         "stage": "templateZipStage",
         "type": "dependency"
      }
   ],
   "name": "purple-gold-spitz-export",
   "stages": [
      {
         "clean_workspace": false,
         "fetch_materials": true,
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "export_windows",
                     "type": "build"
                  }
               ],
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "defaultJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.templates.tpz",
                     "stage": "templateZipStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "linuxJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.linuxbsd.opt.tools.64.llvm",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf templates && unzip \"godot.templates.tpz\" && mkdir pdbs && mv templates/*.pdb pdbs && export VERSION=\"`cat templates/version.txt`\" && export TEMPLATEDIR=\".local/share/godot/templates/$VERSION\" && export BASEDIR=\"`pwd`\" && rm -rf \"$TEMPLATEDIR\" && mkdir -p \"$TEMPLATEDIR\" && cd \"$TEMPLATEDIR\" && mv \"$BASEDIR\"/templates/* ."
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf export_windows && mkdir export_windows && chmod +x godot.linuxbsd.opt.tools.64.llvm && HOME=\"`pwd`\" ./DRI_PRIME=0 xvfb-run --auto-servernum godot.linuxbsd.opt.tools.64.llvm --export \"Windows Desktop\" \"`pwd`\"/export_windows/godot.windows.opt.tools.64.exe --path g -v"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "export_linuxbsd",
                     "type": "build"
                  }
               ],
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "defaultJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.templates.tpz",
                     "stage": "templateZipStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "linuxJob",
                     "pipeline": "godot-template-groups-4-x",
                     "source": "godot.linuxbsd.opt.tools.64.llvm",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf templates && unzip \"godot.templates.tpz\" && mkdir pdbs && mv templates/*.pdb pdbs && export VERSION=\"`cat templates/version.txt`\" && export TEMPLATEDIR=\".local/share/godot/templates/$VERSION\" && export BASEDIR=\"`pwd`\" && rm -rf \"$TEMPLATEDIR\" && mkdir -p \"$TEMPLATEDIR\" && cd \"$TEMPLATEDIR\" && mv \"$BASEDIR\"/templates/* ."
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "rm -rf export_linuxbsd && mkdir export_linuxbsd && chmod +x godot.linuxbsd.opt.tools.64.llvm && HOME=\"`pwd`\" ./DRI_PRIME=0 xvfb-run --auto-servernum godot.linuxbsd.opt.tools.64.llvm --export \"Linuxbsd\" \"`pwd`\"/export_linuxbsd/godot.linuxbsd.opt.tools.64.llvm --path g -v"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "exportStage"
      },
      {
         "clean_workspace": false,
         "jobs": [
            {
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": false,
                     "job": "windowsJob",
                     "pipeline": "purple-gold-spitz-export",
                     "source": "export_windows",
                     "stage": "exportStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_windows ifiregames/purple-gold-spitz:windows-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": false,
                     "job": "linuxDesktopJob",
                     "pipeline": "purple-gold-spitz-export",
                     "source": "export_linuxbsd",
                     "stage": "exportStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_linuxbsd ifiregames/purple-gold-spitz:linux-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "uploadStage"
      }
   ]
}
