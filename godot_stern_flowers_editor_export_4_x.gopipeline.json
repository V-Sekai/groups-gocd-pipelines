{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "stern-flowers"
      }
   ],
   "group": "delta",
   "label_template": "stern-flowers.${godot-template-stern-flowers-4-x_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "ignore_for_scheduling": false,
         "name": "godot-template-stern-flowers-4-x_pipeline_dependency",
         "pipeline": "godot-template-stern-flowers-4-x",
         "stage": "templateZipStage",
         "type": "dependency"
      }
   ],
   "name": "stern-flowers-editor-4-x",
   "stages": [
      {
         "clean_workspace": true,
         "jobs": [
            {
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_windows",
                     "is_source_a_file": true,
                     "job": "windowsJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot.windows.opt.tools.64.exe",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_windows",
                     "is_source_a_file": true,
                     "job": "windowsJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot.windows.opt.tools.64.exe",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_windows",
                     "is_source_a_file": true,
                     "job": "windowsJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot.windows.opt.tools.64.pdb",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_windows $ITCHIO_LOGIN:windows-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_linuxbsd",
                     "is_source_a_file": true,
                     "job": "linuxJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot.linuxbsd.opt.tools.64",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_linuxbsd",
                     "is_source_a_file": true,
                     "job": "linuxJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot.linuxbsd.opt.tools.64",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_linuxbsd $ITCHIO_LOGIN:x11-master --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "macosJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_macos",
                     "is_source_a_file": true,
                     "job": "macosJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot_macos.zip",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_macos",
                     "is_source_a_file": true,
                     "job": "macosJob",
                     "pipeline": "godot-template-stern-flowers-4-x",
                     "source": "godot_macos.zip",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_macos $ITCHIO_LOGIN:macos --userversion $GO_PIPELINE_LABEL-`date --iso=seconds --utc`"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "uploadStage"
      }
   ]
}
