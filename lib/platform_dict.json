        "chmod +x macos_template.app/Contents/MacOS/godot_macos*",
        "rm -rf bin/macos.zip",
        "cd bin && zip -1 -r macos.zip macos_template.app/",
        "cd .. && rm -rf Godot.app && cp -r ./g/misc/dist/macos_template.app Godot.app"
