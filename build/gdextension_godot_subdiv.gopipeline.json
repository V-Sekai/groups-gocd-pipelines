{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "gdextension.godot_subdiv"
      }
   ],
   "group": "echo",
   "label_template": "gdextension.godot_subdiv.${gdextension-cpp_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "name": "gdextension-cpp_pipeline_dependency",
         "pipeline": "gdextension-cpp",
         "stage": "godotCppStage",
         "type": "dependency"
      },
      {
         "branch": "main",
         "destination": "p",
         "ignore_for_scheduling": false,
         "name": "p",
         "shallow_clone": false,
         "type": "git",
         "url": "https://github.com/tefusion/godot-subdiv.git"
      }
   ],
   "name": "gdextension-godot-subdiv",
   "stages": [
      {
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "p/projects/addons/godot_subdiv/bin/libgodot_subdiv.windows.template_debug.x86_64.dll",
                     "type": "build"
                  }
               ],
               "environment_variables": [ ],
               "name": "windows_job",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "job": "windows_job",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-cpp",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-cpp",
                     "job": "windows_job",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-headers",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "cp -a godot-cpp/. p/godot-cpp"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "mkdir -p ../.cicd_cache && PATH=/opt/llvm-mingw/bin:$PATH SCONS_CACHE=../.cicd_cache scons werror=no platform=windows target=editor debug_symbols=yes use_lto=no deprecated=no precision=double use_mingw=yes use_llvm=yes use_thinlto=no warnings=no LINKFLAGS=-Wl,-pdb= CCFLAGS='-Wall -Wno-tautological-compare -g -gcodeview' debug_symbols=no"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "p"
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "p/projects/addons/godot_subdiv/bin/libgodot_subdiv.windows.template_debug.x86_64.dll",
                     "type": "build"
                  }
               ],
               "environment_variables": [ ],
               "name": "linux_job",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "job": "linux_job",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-cpp",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-cpp",
                     "job": "linux_job",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-headers",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "cp -a godot-cpp/. p/godot-cpp"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "mkdir -p ../.cicd_cache && SCONS_CACHE=../.cicd_cache scons werror=no platform=linux target=editor debug_symbols=yes use_lto=no deprecated=no precision=double use_static_cpp=yes use_llvm=yes builtin_freetype=yes"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "p"
                  }
               ]
            }
         ],
         "name": "gdextensionBuildStage"
      }
   ]
}
