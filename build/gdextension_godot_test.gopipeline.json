{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "gdextension.godot_test"
      }
   ],
   "group": "gamma",
   "label_template": "gdextension.godot_test.${gdextension-cpp_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "name": "gdextension-cpp_pipeline_dependency",
         "pipeline": "gdextension-cpp",
         "stage": "godotCppStage",
         "type": "dependency"
      },
      {
         "branch": "master",
         "destination": "p",
         "ignore_for_scheduling": false,
         "name": "p",
         "shallow_clone": false,
         "type": "git",
         "url": "https://github.com/godotengine/godot-cpp.git"
      }
   ],
   "name": "gdextension-godot-test",
   "stages": [
      {
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "p/game/summator/libgdsummator.windows.debug.64.dll",
                     "type": "build"
                  }
               ],
               "environment_variables": [ ],
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "job": "windowsJob",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-cpp",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-cpp",
                     "job": "windowsJob",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-headers",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "cp -a godot-cpp/. p/godot-cpp"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "PATH=/opt/llvm-mingw/bin:$PATH scons werror=no platform=windows target=debug -j`nproc` use_lto=no deprecated=no use_mingw=yes use_llvm=yes use_thinlto=no warnings=no LINKFLAGS=-Wl,-pdb= CCFLAGS='-Wall -Wno-tautological-compare -g -gcodeview' debug_symbols=no"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "p"
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "p/game/summator/libgdsummator.linux.debug.64.so",
                     "type": "build"
                  }
               ],
               "environment_variables": [ ],
               "name": "linuxJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "job": "linuxJob",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-cpp",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-cpp",
                     "job": "linuxJob",
                     "pipeline": "gdextension-cpp",
                     "source": "godot-headers",
                     "stage": "godotCppStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "cp -a godot-cpp/. p/godot-cpp"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  },
                  {
                     "arguments": [
                        "-c",
                        "scons werror=no platform=linux target=debug -j`nproc` use_lto=no deprecated=no use_static_cpp=yes use_llvm=yes builtin_freetype=yes"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "p"
                  }
               ]
            }
         ],
         "name": "gdextensionBuildStage"
      }
   ]
}
