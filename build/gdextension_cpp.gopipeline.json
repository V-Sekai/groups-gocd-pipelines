{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "gdextension.godot-cpp"
      }
   ],
   "group": "echo",
   "label_template": "gdextension.godot-cpp.${godot-groups-editor_pipeline_dependency}.${COUNT}",
   "materials": [
      {
         "name": "godot-groups-editor_pipeline_dependency",
         "pipeline": "godot-groups-editor",
         "stage": "defaultStage",
         "type": "dependency"
      },
      {
         "branch": "master",
         "destination": "godot-cpp",
         "ignore_for_scheduling": false,
         "name": "godot-cpp",
         "shallow_clone": false,
         "type": "git",
         "url": "https://github.com/godotengine/godot-cpp.git"
      },
      {
         "branch": "master",
         "destination": "godot-headers",
         "ignore_for_scheduling": false,
         "name": "godot-headers",
         "shallow_clone": false,
         "type": "git",
         "url": "https://github.com/godotengine/godot-headers.git"
      }
   ],
   "name": "gdextension-cpp",
   "stages": [
      {
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "",
                     "source": "extension_api.json",
                     "type": "build"
                  }
               ],
               "name": "generateApiJsonJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "",
                     "is_source_a_file": true,
                     "job": "linux_job",
                     "pipeline": "godot-groups-editor",
                     "source": "godot.linuxbsd.editor.double.x86_64.llvm",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "chmod +x godot.linuxbsd.editor.double.x86_64.llvm && ./godot.linuxbsd.editor.double.x86_64.llvm --headless --dump-extension-api extension_api.json || true"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "generateApiJsonStage"
      },
      {
         "jobs": [
            {
               "artifacts": [
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/include",
                     "type": "build"
                  },
                  {
                     "destination": "",
                     "source": "godot-headers",
                     "type": "build"
                  },
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/gen/include",
                     "type": "build"
                  },
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/bin",
                     "type": "build"
                  }
               ],
               "name": "windows_job",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-headers",
                     "is_source_a_file": true,
                     "job": "generateApiJsonJob",
                     "pipeline": "gdextension-cpp",
                     "source": "extension_api.json",
                     "stage": "generateApiJsonStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "mkdir -p ../.cicd_cache && PATH=/opt/llvm-mingw/bin:$PATH SCONS_CACHE=../.cicd_cache scons werror=no platform=windows target=template_debug debug_symbols=yes use_lto=no deprecated=no generate_bindings=yes headers_dir=../godot-headers precision=double use_mingw=yes use_llvm=yes use_thinlto=no warnings=no LINKFLAGS=-Wl,-pdb= CCFLAGS='-Wall -Wno-tautological-compare -g -gcodeview' debug_symbols=no"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "godot-cpp"
                  }
               ]
            },
            {
               "artifacts": [
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/include",
                     "type": "build"
                  },
                  {
                     "destination": "",
                     "source": "godot-headers",
                     "type": "build"
                  },
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/gen/include",
                     "type": "build"
                  },
                  {
                     "destination": "godot-cpp",
                     "source": "godot-cpp/bin",
                     "type": "build"
                  }
               ],
               "name": "linux_job",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "godot-headers",
                     "is_source_a_file": true,
                     "job": "generateApiJsonJob",
                     "pipeline": "gdextension-cpp",
                     "source": "extension_api.json",
                     "stage": "generateApiJsonStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "mkdir -p ../.cicd_cache && SCONS_CACHE=../.cicd_cache scons werror=no platform=linux target=template_debug debug_symbols=yes use_lto=no deprecated=no generate_bindings=yes headers_dir=../godot-headers precision=double use_static_cpp=yes use_llvm=yes builtin_freetype=yes"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": "godot-cpp"
                  }
               ]
            }
         ],
         "name": "godotCppStage"
      }
   ]
}
