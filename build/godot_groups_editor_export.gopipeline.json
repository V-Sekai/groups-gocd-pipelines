{
   "environment_variables": [
      {
         "name": "GODOT_STATUS",
         "value": "groups-4.0"
      }
   ],
   "group": "gamma",
   "label_template": "groups-editor-4-0.${COUNT}",
   "materials": [
      {
         "ignore_for_scheduling": false,
         "name": "godot-template-groups-4-0-rev-02_pipeline_dependency",
         "pipeline": "godot-template-groups-4-0-rev-02",
         "stage": "templateZipStage",
         "type": "dependency"
      }
   ],
   "name": "groups-editor-4-0",
   "stages": [
      {
         "clean_workspace": true,
         "jobs": [
            {
               "name": "windowsJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_windows",
                     "is_source_a_file": true,
                     "job": "windowsJob",
                     "pipeline": "godot-template-groups-4-0-rev-02",
                     "source": "godot.windows.opt.tools.64.exe",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_windows",
                     "is_source_a_file": true,
                     "job": "windowsJob",
                     "pipeline": "godot-template-groups-4-0-rev-02",
                     "source": "godot.windows.opt.tools.64.pdb",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_windows ifiregames/chibifire-godot-4-custom-engine:windows-master --userversion `date +\"%Y-%m-%dT%H%M%SZ\" --utc`-$GO_PIPELINE_NAME"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            },
            {
               "name": "linuxDesktopJob",
               "resources": [
                  "linux",
                  "mingw5"
               ],
               "tasks": [
                  {
                     "artifact_origin": "gocd",
                     "destination": "export_linuxbsd",
                     "is_source_a_file": true,
                     "job": "linuxJob",
                     "pipeline": "godot-template-groups-4-0-rev-02",
                     "source": "godot.linuxbsd.opt.tools.64.llvm",
                     "stage": "defaultStage",
                     "type": "fetch"
                  },
                  {
                     "arguments": [
                        "-c",
                        "butler push export_linuxbsd ifiregames/chibifire-godot-4-custom-engine:linux-master --userversion `date +\"%Y-%m-%dT%H%M%SZ\" --utc`-$GO_PIPELINE_NAME"
                     ],
                     "command": "/bin/bash",
                     "type": "exec",
                     "working_directory": ""
                  }
               ]
            }
         ],
         "name": "uploadStage"
      }
   ],
   "timer": {
      "only_on_changes": true,
      "spec": "* * * * * ?"
   }
}
